<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">


    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Adulting101"
        tools:targetApi="31">
        <activity
            android:name=".ui.theme.QuizProcess"
            android:exported="false" />
        <!-- MainActivity Declaration -->
        <activity
            android:name=".ui.theme.MainActivity"
            android:exported="true"
            android:theme="@style/Theme.Adulting101">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity> <!-- CoursesActivity Declaration -->
        <activity
            android:name=".ui.theme.CoursesActivity"
            android:exported="false"
            android:theme="@style/Theme.Adulting101" /> <!-- FAQActivity Declaration -->
        <activity
            android:name=".ui.theme.FAQActivity"
            android:exported="false"
            android:theme="@style/Theme.Adulting101" /> <!-- ToDoActivity Declaration -->
        <activity
            android:name=".ui.theme.ToDoActivity"
            android:exported="false"
            android:theme="@style/Theme.Adulting101" /> <!-- QuizActivity Declaration -->
        <activity
            android:name=".ui.theme.QuizActivity"
            android:exported="true"
            android:theme="@style/Theme.Adulting101" />
    </application>

</manifest>